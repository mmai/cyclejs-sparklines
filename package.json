  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.3",
